{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "UpdateAndDescribeServicesOnCluster",
      "Effect": "Allow",
      "Action": [
        "ecs:UpdateService",
        "ecs:DescribeServices",
        "ecs:ListServices"
      ],
      "Resource": "arn:aws:ecs:us-east-1:567578056281:service/orgjet/*"
    },
    {
      "Sid": "DescribeCluster",
      "Effect": "Allow",
      "Action": "ecs:DescribeClusters",
      "Resource": "arn:aws:ecs:us-east-1:567578056281:cluster/orgjet"
    },
    {
      "Sid": "ReadTaskDefinitions",
      "Effect": "Allow",
      "Action": [
        "ecs:DescribeTaskDefinition",
        "ecs:ListTaskDefinitions"
      ],
      "Resource": "*"
    }
  ]
}
